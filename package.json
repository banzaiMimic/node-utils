{
  "name": "node-utils",
  "version": "0.0.1",
  "description": "utilities for node / express / logging & mongo",
  "scripts": {
    "compile": "tsc && tsc --build tsconfig.es5.json",
    "watch:build": "webpack --mode=production --watch",
    "build": "webpack --mode=production",
    "prepublish": "yarn build",
    "test": "standard && jest --passWithNoTests",
    "dev": "nodemon demo/index.js"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:banzaiMimic/node-utils.git"
  },
  "author": "banzaiMimic",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.8.2",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.20",
    "standard": "^17.0.0",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "request-ip": "^2.2.0",
    "winston": "^3.8.2"
  }
}
